  "devDependencies": {
    "concurrently": "^5.3.0",
    "cpy-cli": "^4.1.0",
    "esbuild": "^0.14.39"
  },
  "dependencies": {
    "botframework-directlinejs": "^0.11.6",
    "classnames": "^2.2.6",
    "core-js": "^3.22.7",
    "emotion": "^10.0.27",
    "jwt-decode": "^3.1.2",
